<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:u="sap.ui.unified"
  xmlns:core="sap.ui.core">
  <Dialog id="importCandlesDialog"
    title="Import Candles">
    <content>
      <u:Calendar id="calendar"
        select="onCalendarSelect"
        intervalSelection="true"/>
    </content>
    <beginButton>
      <Button text="OK" press=".onImportCandlesDialogOk"/>
    </beginButton>
    <endButton>
      <Button text="Cancel" press=".onImportCandlesDialogCancel"/>
    </endButton>
  </Dialog>
</core:FragmentDefinition>
